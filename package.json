{
  "name": "data-from-clipboard",
  "version": "0.3.0",
  "description": "A DataSupplier plugin",
  "engines": {
    "sketch": ">=3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Saint-loup/data-from-clipboard.git"
  },
  "skpm": {
    "name": "data-from-clipboard",
    "manifest": "src/manifest.json",
    "main": "data-from-clipboard.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  }
}
